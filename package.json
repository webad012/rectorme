{
  "name": "rectorme",
	"publisher": "milosjankovic",
  "displayName": "Rector Me",
  "description": "Rector Me",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/webad012/vscrectorme.git"
  },
  "engines": {
    "vscode": "^1.83.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "RectorMe",
      "properties": {
        "rectorme.rectorPath": {
          "type": "string",
          "description": "Path of Rector dir (containing vendor/bin/rector and rector.php)"
        }
      }
    },
    "commands": [
      {
        "command": "rectorme.rectorme",
        "title": "Rector Me"
      }
    ],
    "menus": {
			"commandPalette": [
				{
					"command": "rectorme.rectorme",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "rectorme.rectorme",
					"group": "RectorMe",
					"when": "true"
				}
			]
		}
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.1",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
